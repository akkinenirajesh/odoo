xml
<buvi>
  <model id="Stock.StockQuant" name="StockQuant" description="Quants">
    <field name="ProductId" type="Product.Product" label="Product" required="true" index="true" checkCompany="true" />
    <field name="ProductTemplateId" type="Product.Template" label="Product Template" related="ProductId.ProductTemplateId" />
    <field name="ProductUomId" type="Uom.Uom" label="Unit of Measure" readonly="true" related="ProductId.UomId" />
    <field name="IsFavorite" type="bool" related="ProductTemplateId.IsFavorite" />
    <field name="CompanyId" type="Core.Company" label="Company" related="LocationId.CompanyId" store="true" readonly="true" />
    <field name="LocationId" type="Stock.Location" label="Location" autoJoin="true" ondelete="restrict" required="true" index="true" />
    <field name="WarehouseId" type="Stock.Warehouse" related="LocationId.WarehouseId" />
    <field name="StorageCategoryId" type="Stock.StorageCategory" related="LocationId.StorageCategoryId" store="true" />
    <field name="CyclicInventoryFrequency" type="integer" related="LocationId.CyclicInventoryFrequency" />
    <field name="LotId" type="Stock.Lot" label="Lot/Serial Number" index="true" ondelete="restrict" checkCompany="true" />
    <field name="LotProperties" type="Properties" related="LotId.LotProperties" definition="ProductId.LotPropertiesDefinition" readonly="true" />
    <field name="SnDuplicated" type="bool" label="Duplicated Serial Number" compute="ComputeSnDuplicated" help="If the same SN is in another Quant" />
    <field name="PackageId" type="Stock.QuantPackage" label="Package" domain="[('LocationId', '=', LocationId)]" help="The package containing this quant" ondelete="restrict" checkCompany="true" index="true" />
    <field name="OwnerId" type="Res.Partner" label="Owner" help="This is the owner of the quant" checkCompany="true" index="btree_not_null" />
    <field name="Quantity" type="float" label="Quantity" help="Quantity of products in this quant, in the default unit of measure of the product" readonly="true" digits="Product Unit of Measure" />
    <field name="ReservedQuantity" type="float" label="Reserved Quantity" default="0.0" help="Quantity of reserved products in this quant, in the default unit of measure of the product" readonly="true" required="true" digits="Product Unit of Measure" />
    <field name="AvailableQuantity" type="float" label="Available Quantity" help="On hand quantity which hasn't been reserved on a transfer, in the default unit of measure of the product" compute="ComputeAvailableQuantity" digits="Product Unit of Measure" />
    <field name="InDate" type="datetime" label="Incoming Date" readonly="true" required="true" default="Now" />
    <field name="Tracking" type="selection" related="ProductId.Tracking" readonly="true" />
    <field name="OnHand" type="bool" label="On Hand" store="false" search="SearchOnHand" />
    <field name="ProductCategId" type="Product.Category" related="ProductTemplateId.CategId" />
    <field name="InventoryQuantity" type="float" label="Counted Quantity" digits="Product Unit of Measure" help="The product's counted quantity." />
    <field name="InventoryQuantityAutoApply" type="float" label="Inventoried Quantity" digits="Product Unit of Measure" compute="ComputeInventoryQuantityAutoApply" inverse="SetInventoryQuantity" groups="stock.group_stock_manager" />
    <field name="InventoryDiffQuantity" type="float" label="Difference" compute="ComputeInventoryDiffQuantity" store="true" help="Indicates the gap between the product's theoretical quantity and its counted quantity." readonly="true" digits="Product Unit of Measure" />
    <field name="InventoryDate" type="date" label="Scheduled Date" compute="ComputeInventoryDate" store="true" readonly="false" help="Next date the On Hand Quantity should be counted." />
    <field name="LastCountDate" type="date" label="Last Count Date" compute="ComputeLastCountDate" help="Last time the Quantity was Updated" />
    <field name="InventoryQuantitySet" type="bool" label="Inventory Quantity Set" store="true" compute="ComputeInventoryQuantitySet" readonly="false" default="false" />
    <field name="IsOutdated" type="bool" label="Quantity has been moved since last count" compute="ComputeIsOutdated" search="SearchIsOutdated" />
    <field name="UserId" type="Res.Users" label="Assigned To" help="User assigned to do product count." />
  </model>

  <optionset id="Stock.RemovalStrategy" name="Removal Strategy">
    <option value="fifo" name="FIFO" />
    <option value="lifo" name="LIFO" />
    <option value="least_packages" name="Least Packages" />
    <option value="closest" name="Closest" />
  </optionset>

  <model id="Stock.QuantPackage" name="QuantPackage" description="Packages">
    <field name="Name" type="string" label="Package Reference" copy="false" index="trigram" required="true" default="SequenceNextByCode('stock.quant.package')" />
    <field name="QuantIds" type="Stock.Quant" label="Bulk Content" collection="true" readonly="true" domain="['|', ('Quantity', '!=', 0), ('ReservedQuantity', '!=', 0)]" />
    <field name="PackageTypeId" type="Stock.PackageType" label="Package Type" index="true" />
    <field name="LocationId" type="Stock.Location" label="Location" compute="ComputePackageInfo" index="true" readonly="false" store="true" />
    <field name="CompanyId" type="Core.Company" label="Company" compute="ComputePackageInfo" index="true" readonly="true" store="true" />
    <field name="OwnerId" type="Res.Partner" label="Owner" compute="ComputeOwnerId" search="SearchOwner" readonly="true" computeSudo="true" />
    <field name="PackageUse" type="selection" label="Package Use" default="disposable" required="true" >
      <option value="disposable" name="Disposable Box" />
      <option value="reusable" name="Reusable Box" />
    </field>
    <field name="ValidSSCC" type="bool" label="Package name is valid SSCC" compute="ComputeValidSSCC" />
    <field name="PackDate" type="date" label="Pack Date" default="Today" />
  </model>
</buvi>
