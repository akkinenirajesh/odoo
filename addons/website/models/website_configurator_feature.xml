xml
<buvi>
  <model id="Website.WebsiteConfiguratorFeature" name="WebsiteConfiguratorFeature" description="Website Configurator Feature">
    <field name="Sequence" type="integer" />
    <field name="Name" type="string" label="Name" translate="true" />
    <field name="Description" type="string" label="Description" translate="true" />
    <field name="Icon" type="string" />
    <field name="IapPageCode" type="string" help="Page code used to tell IAP website_service for which page a snippet list should be generated" />
    <field name="WebsiteConfigPreselection" type="string" help="Comma-separated list of website type/purpose for which this feature should be pre-selected" />
    <field name="PageView" type="ir.ui.view" collection="true" />
    <field name="Module" type="ir.module.module" collection="true" />
    <field name="FeatureUrl" type="string" />
    <field name="MenuSequence" type="integer" help="If set, a website menu will be created for the feature." />
    <field name="MenuCompany" type="bool" help="If set, add the menu as a second level menu, as a child of 'Company' menu." />
  </model>
</buvi>
