xml
<buvi>
    <model id="Stock.Move" name="StockMove" description="Stock Move Ewaybill">
        <field name="L10nInEwaybillId" type="Stock.Ewaybill" relation="one2many" related="PickingId.L10nInEwaybillId" />
        <field name="CompanyCurrencyId" type="Account.Currency" relation="many2one" related="CompanyId.CurrencyId" />
        <field name="EwaybillPriceUnit" type="decimal" label="Ewaybill Price Unit" currency="CompanyCurrencyId" store="true" compute="ComputeL10nInEwaybillPriceUnit" />
        <field name="EwaybillTaxIds" type="Account.Tax" relation="many2many" label="Taxes" store="true" compute="ComputeL10nInTaxIds" />
    </model>
</buvi>
