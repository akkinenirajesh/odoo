xml
<buvi>
  <model id="Pos.PosCategory" name="PosCategory" description="Point of Sale Category">
    <field name="Name" type="string" label="Category Name" required="true" translate="true" />
    <field name="ParentID" type="Pos.PosCategory" label="Parent Category" collection="true" />
    <field name="Image" type="image" label="Image" />
    <field name="Sequence" type="integer" label="Sequence" />
    <field name="HourUntil" type="float" label="Availability Until" default="24.0" help="The product will be available until this hour." />
    <field name="HourAfter" type="float" label="Availability After" default="0.0" help="The product will be available after this hour." />
  </model>
</buvi>
