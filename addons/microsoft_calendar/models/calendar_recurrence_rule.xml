<buvi>
    <model id="Calendar.RecurrenceRule" name="Calendar.RecurrenceRule" description="Calendar Recurrence Rule">
        <field name="NeedSyncM" type="bool" default="false" />
        <field name="Rrule" type="string" label="Recurrence Rule" />
        <field name="Dtstart" type="datetime" label="Start Date" />
        <field name="CalendarEventId" type="Calendar.Event"  relation="many2one" label="Calendar Event"  collection="true" />
        <field name="BaseEventId" type="Calendar.Event"  relation="many2one" label="Base Event"  collection="true" />
        <field name="EventTz" type="string" label="Event Timezone" />
        <field name="MicrosoftId" type="string" label="Microsoft Id" />
        <field name="MsUniversalEventId" type="string" label="Microsoft Universal Event Id" />
    </model>
</buvi>
