xml
<buvi>
    <model id="Mail.MailPushDevice" name="MailPushDevice" description="Push Notification Device">
        <field name="PartnerId" type="Res.Partner" label="Partner" required="true" />
        <field name="Endpoint" type="string" label="Browser endpoint" required="true" />
        <field name="Keys" type="string" label="Browser keys" required="true" help="It's refer to browser keys used by the notification: 
- p256dh: It's the subscription public key generated by the browser. The browser will 
          keep the private key secret and use it for decrypting the payload
- auth: The auth value should be treated as a secret and not shared outside of Odoo" />
        <field name="ExpirationTime" type="datetime" label="Expiration Token Date" />
        <unique>
            <field name="Endpoint" />
            <message>The endpoint must be unique !</message>
        </unique>
    </model>
</buvi>
