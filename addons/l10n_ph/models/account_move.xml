xml
<buvi>
    <model id="Account.AccountMove" name="AccountMove" description="Account Move">
        <field name="MoveType" type="string" label="Move Type" />
        <field name="MoveLines" type="Account.AccountMoveLine" collection="true" />
    </model>
    <model id="Account.AccountMoveLine" name="AccountMoveLine" description="Account Move Line">
        <field name="Name" type="string" label="Name" />
        <field name="Debit" type="decimal" label="Debit" />
        <field name="Credit" type="decimal" label="Credit" />
        <field name="AccountID" type="Account.Account" />
    </model>
    <model id="Account.Account" name="Account" description="Account">
        <field name="Code" type="string" label="Code" />
        <field name="Name" type="string" label="Name" />
    </model>
</buvi>
