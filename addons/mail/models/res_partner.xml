xml
<buvi>
    <model id="Mail.ResPartner" name="Mail.ResPartner" description="Partner">
        <field name="Email" type="string" tracking="1" />
        <field name="Phone" type="string" tracking="2" />
        <field name="Parent" type="Mail.ResPartner" relation="true" tracking="3" />
        <field name="User" type="Res.Users" relation="true" tracking="4" />
        <field name="Vat" type="string" tracking="5" />
        <field name="ContactAddressInline" type="string" compute="ComputeContactAddressInline" label="Inlined Complete Address" tracking="true" />
        <field name="StarredMessageIds" type="Mail.Message" relation="true" collection="true" />
        <field name="NeedactionCount" type="integer" compute="ComputeNeedactionCount" />
    </model>

    <optionset id="Mail.ResPartnerImStatus" name="Partner IM Status">
        <option value="online" name="Online" />
        <option value="away" name="Away" />
        <option value="dnd" name="Do Not Disturb" />
        <option value="offline" name="Offline" />
        <option value="bot" name="Bot" />
    </optionset>
</buvi>
