xml
<buvi>
  <model id="Stock.Partner" name="Partner" description="Partner">
    <field name="PropertyStockCustomer" type="Stock.Location" label="Customer Location" companyDependent="true" checkCompany="true" 
            domain="[&#x27;|&#x27;,(&#x27;CompanyId&#x27;,&#x27;=&#x27;,False),(&#x27;CompanyId&#x27;,&#x27;=&#x27;,AllowedCompanyIds[0])]">
      <help>The stock location used as destination when sending goods to this contact.</help>
    </field>
    <field name="PropertyStockSupplier" type="Stock.Location" label="Vendor Location" companyDependent="true" checkCompany="true"
            domain="[&#x27;|&#x27;,(&#x27;CompanyId&#x27;,&#x27;=&#x27;,False),(&#x27;CompanyId&#x27;,&#x27;=&#x27;,AllowedCompanyIds[0])]">
      <help>The stock location used as source when receiving goods from this contact.</help>
    </field>
    <field name="PickingWarn" type="Stock.PickingWarn" label="Stock Picking" help="WARNING_HELP" default="NoMessage" />
    <field name="PickingWarnMsg" type="text" label="Message for Stock Picking" />
  </model>

  <optionset id="Stock.PickingWarn" name="Stock Picking Warn">
    <option value="NoMessage" name="No Message" />
    <option value="Warning" name="Warning" />
    <option value="Block" name="Block" />
  </optionset>
</buvi>
