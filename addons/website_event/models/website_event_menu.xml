xml
<buvi>
    <model id="Website.EventMenu" name="Website.EventMenu" description="Website Event Menu">
        <field name="MenuId" type="Website.Menu" label="Menu" required="true" ondelete="cascade" />
        <field name="EventId" type="Event.Event" label="Event" required="true" ondelete="cascade" />
        <field name="ViewId" type="Ir.Ui.View" label="View" required="true" ondelete="cascade" help="Used when not being an url based menu" />
        <field name="MenuType" type="Website.EventMenu.MenuType" label="Menu Type" required="true" />
    </model>

    <optionset id="Website.EventMenu.MenuType" name="Menu Type">
        <option value="Community" name="Community Menu" />
        <option value="Introduction" name="Introduction" />
        <option value="Location" name="Location" />
        <option value="Register" name="Info" />
    </optionset>
</buvi>
