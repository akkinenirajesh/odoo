    <field name="ImageSmall" type="binary" />
    <field name="Vat" type="char" />
    <field name="IsCompany" type="bool" />
    <field name="IsEmployee" type="bool" />
    <field name="ParentId" type="Many2One" relation="Res.Users" />
    <field name="FiscalPositionId" type="Many2One" relation="Account.FiscalPosition" />
    <field name="Country" type="Many2One" relation="Res.Country" />
    <field name="Industry" type="Selection" selection="ResPartner.GetIndustriesSelection" />
    <field name="Customer" type="bool" />
    <field name="Supplier" type="bool" />
    <field name="Comment" type="text" />
    <field name="Street3" type="string" />
    <field name="MobilePhone" type="string" />
    <field name="Fax" type="string" />
