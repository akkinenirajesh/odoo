xml
<buvi>
    <model id="Hr.Employee" name="Employee" description="Employee">
        <field name="Vehicle" type="string" label="Company Vehicle" />
        <field name="ContractIds" type="Hr.Contract" multiple="true" label="Employee Contracts" />
        <field name="ContractId" type="Hr.Contract" label="Current Contract" 
               help="Current contract of the employee" />
        <field name="CalendarMismatch" type="bool" label="Calendar Mismatch" />
        <field name="ContractsCount" type="integer" label="Contract Count" />
        <field name="ContractWarning" type="bool" label="Contract Warning" />
        <field name="FirstContractDate" type="date" label="First Contract Date" />
    </model>
</buvi>
