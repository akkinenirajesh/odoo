xml
<buvi>
  <model id="Account.AccountMove" name="AccountMove" description="Account Move">
    <field name="PurchaseVendorBillId" type="Purchase.BillUnion" label="Auto-complete" store="false" readonly="false"
      help="Auto-complete from a past bill / purchase order." />
    <field name="PurchaseId" type="Purchase.Order" label="Purchase Order" store="false" readonly="false"
      help="Auto-complete from a past purchase order." />
    <field name="PurchaseOrderCount" type="integer" label="Purchase Order Count" compute="ComputePurchaseOrderCount" />
    <field name="InvoiceOrigin" type="string" label="Invoice Origin" />
    <field name="Ref" type="string" label="Reference" />
    <field name="PaymentReference" type="string" label="Payment Reference" />
    <field name="PartnerId" type="Res.Partner" label="Partner" />
    <field name="CompanyId" type="Res.Company" label="Company" />
    <field name="CurrencyId" type="Res.Currency" label="Currency" />
    <field name="JournalId" type="Account.Journal" label="Journal" />
    <field name="MoveType" type="selection" label="Move Type"
      selection="[(&#39;entry&#39;, &#39;Journal Entry&#39;), (&#39;out_invoice&#39;, &#39;Customer Invoice&#39;), (&#39;out_refund&#39;, &#39;Customer Credit Note&#39;), (&#39;in_invoice&#39;, &#39;Vendor Bill&#39;), (&#39;in_refund&#39;, &#39;Vendor Credit Note&#39;), (&#39;out_receipt&#39;, &#39;Customer Receipt&#39;), (&#39;in_receipt&#39;, &#39;Vendor Payment&#39;)]" />
    <field name="ReversedEntryId" type="Account.Move" label="Reversed Entry" />
    <field name="InvoiceVendorBillId" type="Account.Move" label="Vendor Bill" />
    <field name="InvoiceLineIds" collection="true" type="Account.MoveLine" label="Invoice Lines" />
    <field name="LineIds" collection="true" type="Account.MoveLine" label="Lines" />
    <method name="OnchangePurchaseAutoComplete" onchange="true" >
      <field name="PurchaseVendorBillId" />
      <field name="PurchaseId" />
    </method>
    <method name="OnchangePartnerId" onchange="true" >
      <field name="PartnerId" />
      <field name="CompanyId" />
    </method>
    <method name="ComputePurchaseOrderCount" compute="true" >
      <field name="LineIds" />
    </method>
    <method name="ActionViewSourcePurchaseOrders" action="true" />
    <method name="Create" create="true" />
    <method name="Write" write="true" />
    <method name="FindMatchingSubsetPOLines" static="true" />
    <method name="FindMatchingPOAndInvLines" static="true" />
    <method name="SetPurchaseOrders" />
    <method name="MatchPurchaseOrders" static="true" />
    <method name="FindAndSetPurchaseOrders" />
    <method name="_GetEDICreation" static="true" />
  </model>
  <model id="Account.MoveLine" name="AccountMoveLine" description="Account Move Line">
    <field name="PurchaseLineId" type="Purchase.OrderLine" label="Purchase Order Line" ondelete="set null" />
    <field name="PurchaseOrderId" type="Purchase.Order" label="Purchase Order" related="PurchaseLineId.OrderId" readonly="true" />
    <method name="CopyDataExtendBusinessFields" />
  </model>
</buvi>
