xml
<buvi>
  <model id="Stock.RemovalStrategy" name="RemovalStrategy" description="Removal Strategy">
    <field name="Name" type="string" label="Name" required="true" translate="true" />
    <field name="Method" type="string" label="Method" required="true" translate="true" help="FIFO, LIFO..." />
  </model>
  <model id="Stock.StockPutawayRule" name="StockPutawayRule" description="Putaway Rule" order="Sequence,ProductId">
    <field name="ProductId" type="Stock.Product" label="Product" check_company="true"
      domain="[('ProductTmplId', '=', context.get('active_id', False))] if context.get('active_model') == 'Product.Template' else [('Type', '!=', 'service')]"
      ondelete="cascade" collection="true" />
    <field name="CategoryId" type="Product.Category" label="Product Category"
      domain="[('FilterForStockPutawayRule', '=', True)]" ondelete="cascade" collection="true" />
    <field name="LocationInId" type="Stock.Location" label="When product arrives in" check_company="true"
      domain="[('ChildIds', '!=', False)]" required="true" ondelete="cascade" index="true" collection="true" />
    <field name="LocationOutId" type="Stock.Location" label="Store to sublocation" check_company="true"
      domain="[('Id', 'child_of', LocationInId)]" required="true" ondelete="cascade" collection="true" />
    <field name="Sequence" type="integer" label="Priority" help="Give to the more specialized category, a higher priority to have them in top of the list." />
    <field name="CompanyId" type="Res.Company" label="Company" required="true"
      default="lambda s: s.Env.Company.Id" index="true" collection="true" />
    <field name="PackageTypeIds" type="Stock.PackageType" label="Package Type" check_company="true" collection="true" />
    <field name="StorageCategoryId" type="Stock.StorageCategory" label="Storage Category"
      compute="ComputeStorageCategory" store="true" readonly="false" ondelete="cascade" check_company="true" collection="true" />
    <field name="Active" type="bool" label="Active" default="true"
      help="Set active to false to hide the Account Tag without removing it." />
    <field name="Sublocation" type="Stock.Sublocation" label="Sublocation" default="No" />
  </model>
  <optionset id="Stock.Sublocation" name="Sublocation">
    <option value="No" name="No" />
    <option value="LastUsed" name="Last Used" />
    <option value="ClosestLocation" name="Closest Location" />
  </optionset>
</buvi>
