xml
<buvi>
  <model id="Stock.StockWarehouse" name="StockWarehouse" description="Stock Warehouse">
    <field name="RepairTypeId" type="Stock.PickingType" label="Repair Operation Type" relation="Stock.PickingType" required="true" />
    <field name="Active" type="bool" default="true" help="Set active to false to hide the Stock Warehouse without removing it." />
    <field name="Code" type="string" label="Code" />
    <field name="CompanyId" type="Core.Company" relation="Core.Company" />
    <field name="LotStockId" type="Stock.Location" relation="Stock.Location" />
    <field name="Name" type="string" label="Name" />
  </model>
  <model id="Stock.PickingType" name="PickingType" description="Picking Type">
    <field name="Active" type="bool" default="true" help="Set active to false to hide the Picking Type without removing it." />
    <field name="Barcode" type="string" label="Barcode" />
    <field name="Code" type="string" label="Code" />
    <field name="CompanyId" type="Core.Company" relation="Core.Company" />
    <field name="DefaultLocationDestId" type="Stock.Location" relation="Stock.Location" />
    <field name="DefaultLocationSrcId" type="Stock.Location" relation="Stock.Location" />
    <field name="DefaultRecycleLocationDestId" type="Stock.Location" relation="Stock.Location" />
    <field name="DefaultRemoveLocationDestId" type="Stock.Location" relation="Stock.Location" />
    <field name="Name" type="string" label="Name" />
    <field name="Sequence" type="integer" label="Sequence" />
    <field name="SequenceCode" type="string" label="Sequence Code" />
  </model>
</buvi>
