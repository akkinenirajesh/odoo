<?xml version="1.0" encoding="utf-8"?>
<buvi>
  <model id="hr.employee.location" name="Hr.EmployeeLocation" track_created="true" track_update="true">
    <field name="Employee" label="Employee" type="Hr.Employee" required="true" copied="true" on-delete="Cascade"/>
    <field name="WorkLocation" label="Location" type="Hr.WorkLocation" required="true" copied="true" on-delete="Restrict"/>
    <field name="Date" label="Date" type="date" copied="true"/>
    <field name="DayWeekString" label="Day Week String" type="string" readonly="true" transient="true"/>
    <field name="EmployeeName" label="Employee Name" type="string" readonly="true" transient="true" related-field="Employee.Name"/>
    <field name="WorkLocationName" label="Location name" type="string" readonly="true" transient="true" related-field="WorkLocation.Name"/>
  </model>
  <model id="hr.work.location" name="Hr.WorkLocation" track_created="true" track_update="true">
    <field name="Company" label="Company" type="Res.Company" required="true" copied="true" on-delete="Restrict"/>
    <field name="Address" label="Work Address" type="Res.Partner" required="true" copied="true" on-delete="Restrict"/>
    <field name="Active" label="Active" type="bool" copied="true"/>
    <field name="LocationNumber" label="Location Number" type="string" copied="true"/>
    <field name="LocationType" label="Cover Image" type="Hr.WorkLocationCoverImage" required="true" copied="true"/>
    <field name="Name" label="Work Location" type="string" required="true" copied="true"/>
  </model>
  <model id="event.track.location" name="Event.TrackLocation" track_created="true" track_update="true">
    <field name="Name" label="Location" type="string" required="true" copied="true"/>
    <field name="Sequence" label="Sequence" type="integer" copied="true"/>
  </model>
  <model id="stock.location" name="Stock.Location" track_created="true" track_update="true">
    <field name="ValuationInAccount" label="Stock Valuation Account (Incoming)" type="Account.Account" copied="true" on-delete="Clear"/>
    <field name="ValuationOutAccount" label="Stock Valuation Account (Outgoing)" type="Account.Account" copied="true" on-delete="Clear"/>
    <field name="RemovalStrategy" label="Removal Strategy" type="Product.Removal" copied="true" on-delete="Clear"/>
    <field name="Company" label="Company" type="Res.Company" indexed="true" copied="true" on-delete="Clear"/>
    <field name="Subcontractors" label="Subcontractor" type="Res.Partner" collection="true"/>
    <field name="Childs" label="Contains" type="Stock.Location" collection="true"/>
    <field name="ChildInternalLocations" label="Internal locations among descendants" type="Stock.Location" collection="true" readonly="true" transient="true"/>
    <field name="Location" label="Parent Location" type="Stock.Location" indexed="true" copied="true" on-delete="Cascade"/>
    <field name="IncomingMoveLines" label="Incoming Move Line" type="Stock.MoveLine" collection="true"/>
    <field name="OutgoingMoveLines" label="Outgoing Move Line" type="Stock.MoveLine" collection="true"/>
    <field name="PutawayRules" label="Putaway Rules" type="Stock.PutawayRule" collection="true"/>
    <field name="Quants" label="Quant" type="Stock.Quant" collection="true"/>
    <field name="StorageCategory" label="Storage Category" type="Stock.StorageCategory" copied="true" on-delete="Clear"/>
    <field name="Warehouse" label="Warehouse" type="Stock.Warehouse" readonly="true" on-delete="Clear"/>
    <field name="WarehouseViews" label="Warehouse View" type="Stock.Warehouse" collection="true" readonly="true"/>
    <field name="Active" label="Active" type="bool" copied="true"/>
    <field name="Barcode" label="Barcode" type="string"/>
    <field name="Comment" label="Additional Information" type="html" copied="true"/>
    <field name="CompleteName" label="Full Location Name" type="string" readonly="true"/>
    <field name="CyclicInventoryFrequency" label="Inventory Frequency (Days)" type="integer" copied="true"/>
    <field name="ForecastWeight" label="Forecasted Weight" type="float" readonly="true" transient="true"/>
    <field name="IsEmpty" label="Is Empty" type="bool" readonly="true" transient="true"/>
    <field name="IsSubcontractingLocation" label="Is a Subcontracting Location?" type="bool" copied="true"/>
    <field name="LastInventoryDate" label="Last Effective Inventory" type="date" readonly="true" copied="true"/>
    <field name="Name" label="Location Name" type="string" required="true" copied="true"/>
    <field name="NetWeight" label="Net Weight" type="float" readonly="true" transient="true"/>
    <field name="NextInventoryDate" label="Next Expected Inventory" type="date" readonly="true"/>
    <field name="ParentPath" label="Parent Path" type="string" indexed="true" copied="true"/>
    <field name="Posx" label="Corridor (X)" type="integer" copied="true"/>
    <field name="Posy" label="Shelves (Y)" type="integer" copied="true"/>
    <field name="Posz" label="Height (Z)" type="integer" copied="true"/>
    <field name="ReplenishLocation" label="Replenish Location" type="bool"/>
    <field name="ReturnLocation" label="Is a Return Location?" type="bool" copied="true"/>
    <field name="ScrapLocation" label="Is a Scrap Location?" type="bool" copied="true"/>
    <field name="Usage" label="Location Type" type="Stock.InventoryLocationsLocationType" required="true" indexed="true" copied="true"/>
  </model>
  <model id="lunch.location" name="Lunch.Location" track_created="true" track_update="true">
    <field name="Company" label="Company" type="Res.Company" copied="true" on-delete="Clear"/>
    <field name="Address" label="Address" type="text" copied="true"/>
    <field name="Name" label="Location Name" type="string" required="true" copied="true"/>
  </model>
</buvi>
