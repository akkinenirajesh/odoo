xml
<buvi>
    <model id="Website.WebsiteMenu" name="WebsiteMenu" description="Website Menu">
        <field name="Url" type="string" label="URL" />
        <field name="Name" type="string" label="Menu Name" required="true" translate="true" />
        <field name="ParentID" type="Website.WebsiteMenu" label="Parent Menu" collection="true" />
        <field name="Sequence" type="integer" label="Sequence" default="10" />
        <field name="NewWindow" type="bool" label="Open in New Window" default="false" />
        <field name="Active" type="bool" label="Active" default="true" />
    </model>
</buvi>
