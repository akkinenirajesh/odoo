xml
<buvi>
    <model id="Account.AccountMove" name="AccountMove" description="Account Move">
        <field name="L10nItEdiDoiDate" type="date" label="Date on which Declaration of Intent is applied" 
               compute="_ComputeL10nItEdiDoiDate" />
        <field name="L10nItEdiDoiUse" type="bool" label="Use Declaration of Intent" 
               compute="_ComputeL10nItEdiDoiUse" />
        <field name="L10nItEdiDoiId" type="L10nItEdiDoi.DeclarationOfIntent" label="Declaration of Intent" 
               compute="_ComputeL10nItEdiDoiId" store="true" readonly="false" precompute="true" />
        <field name="L10nItEdiDoiAmount" type="decimal" label="Declaration of Intent Amount" 
               compute="_ComputeL10nItEdiDoiAmount" store="true" readonly="true" 
               help="Total amount of sales under the Declaration of Intent of this document" />
        <field name="L10nItEdiDoiWarning" type="string" label="Declaration of Intent Threshold Warning" 
               compute="_ComputeL10nItEdiDoiWarning" />
    </model>
</buvi>
