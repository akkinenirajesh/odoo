xml
<buvi>
    <model id="Membership.Product" name="Membership.Product" description="Product with Membership Feature">
        <field name="Membership" type="bool" label="Membership" help="Check if the product is eligible for membership." />
        <field name="MembershipDateFrom" type="date" label="Membership Start Date" help="Date from which membership becomes active." />
        <field name="MembershipDateTo" type="date" label="Membership End Date" help="Date until which membership remains active." />
        <constraint name="MembershipDateGreater" msg="Error! Ending Date cannot be set before Beginning Date." 
                     check="MembershipDateTo >= MembershipDateFrom" />
    </model>
</buvi>
