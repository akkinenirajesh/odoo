xml
<buvi>
  <model id="Sale.SaleOrderLine" name="Sale.SaleOrderLine" description="Sale Order Line">
    <field name="PurchaseLineIds" type="Sale.SaleOrderLine" collection="true" label="Generated Purchase Lines" readonly="true" help="Purchase line generated by this Sales item on order confirmation, or when the quantity was increased." />
    <field name="PurchaseLineCount" type="integer" label="Number of generated purchase items" />
    <field name="ProductUomQty" type="float" />
    <field name="State" type="selection" />
    <field name="IsExpense" type="bool" />
    <field name="ProductId" type="Product.Product" />
    <field name="QtyDelivered" type="float" />
    <field name="OrderId" type="Sale.SaleOrder" />
    <field name="ProductUom" type="Product.Uom" />
    <field name="Origin" type="Sale.SaleOrderLine" />
    <field name="Company" type="Res.Company" />
    <field name="AnalyticDistribution" type="selection" />
    <field name="CommitmentDate" type="datetime" />
  </model>
  <optionset id="Sale.SaleOrderLine.State" name="Sale Order Line State">
    <option value="sale" name="Sale" />
  </optionset>
</buvi>
