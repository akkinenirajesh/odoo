xml
<buvi>
  <model id="microsoft_calendar.Meeting" name="microsoft_calendar.Meeting" description="Meeting">
    <field name="MicrosoftRecurrenceMasterId" type="string" label="Microsoft Recurrence Master Id" />
    <field name="Name" type="string" label="Name" />
    <field name="Description" type="text" label="Description" />
    <field name="AllDay" type="bool" label="All Day" />
    <field name="Start" type="datetime" label="Start" />
    <field name="DateEnd" type="date" label="Date End" />
    <field name="Stop" type="datetime" label="Stop" />
    <field name="UserId" type="res.users" label="User" />
    <field name="Privacy" type="Selection" label="Privacy" selection="microsoft_calendar.MeetingPrivacy" />
    <field name="AttendeeIds" type="calendar.attendee" collection="true" label="Attendees" />
    <field name="AlarmIds" type="calendar.alarm" collection="true" label="Reminders" />
    <field name="Location" type="string" label="Location" />
    <field name="ShowAs" type="Selection" label="Show As" selection="microsoft_calendar.MeetingShowAs" />
    <field name="Active" type="bool" label="Active" />
    <field name="NeedSyncM" type="bool" label="Need Sync M" />
    <field name="RecurrenceId" type="calendar.recurrence" label="Recurrence" />
    <field name="Reccurrency" type="bool" label="Reccurrency" />
    <field name="FollowRecurrence" type="bool" label="Follow Recurrence" />
    <field name="MicrosoftId" type="string" label="Microsoft Id" />
    <field name="MsUniversalEventId" type="string" label="MS Universal Event Id" />
    <field name="VideocallLocation" type="string" label="Videocall Location" />
  </model>
  <optionset id="microsoft_calendar.MeetingPrivacy" name="Meeting Privacy">
    <option value="public" name="Public" />
    <option value="private" name="Private" />
    <option value="confidential" name="Confidential" />
  </optionset>
  <optionset id="microsoft_calendar.MeetingShowAs" name="Meeting Show As">
    <option value="free" name="Free" />
    <option value="busy" name="Busy" />
  </optionset>
</buvi>
