xml
<buvi>
    <model id="TestAccessRight.SomeObj" name="TestAccessRight.SomeObj" description="Object For Test Access Right">
        <field name="Val" type="integer" />
        <field name="CategId" type="TestAccessRight.ObjCateg" />
        <field name="ParentId" type="TestAccessRight.SomeObj" />
        <field name="CompanyId" type="Res.Company" />
        <field name="Forbidden" type="integer" groups="TestAccessRights.TestGroup,Base.GroupPortal" default="5" />
        <field name="Forbidden2" type="integer" groups="TestAccessRights.TestGroup" />
        <field name="Forbidden3" type="integer" groups="NoAccess" />
    </model>
    <model id="TestAccessRight.Container" name="TestAccessRight.Container" description="Test Access Right Container">
        <field name="SomeIds" type="TestAccessRight.SomeObj" collection="true" />
    </model>
    <model id="TestAccessRight.Inherits" name="TestAccessRight.Inherits" description="Object for testing related access rights">
        <field name="SomeId" type="TestAccessRight.SomeObj" required="true" />
    </model>
    <model id="TestAccessRight.Child" name="TestAccessRight.Child" description="Object for testing company ir rule">
        <field name="ParentId" type="TestAccessRight.SomeObj" />
    </model>
    <model id="TestAccessRight.ObjCateg" name="TestAccessRight.ObjCateg" description="Context dependent searchable model">
        <field name="Name" type="string" required="true" />
    </model>
    <model id="TestAccessRight.FakeTicket" name="TestAccessRight.FakeTicket" description="Fake ticket For Test Access Right">
        <field name="Name" type="string" />
        <field name="MessagePartnerIds" type="Res.Partner" collection="true" />
    </model>
    <model id="Res.Partner" name="Res.Partner" description="Res Partner">
        <field name="CurrencyId" type="Res.Currency" readonly="true" />
        <field name="Monetary" type="monetary" />
    </model>
</buvi>
