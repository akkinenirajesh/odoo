<buvi>
    <model id="Event.EventTicket" name="EventTicket" description="Event Ticket">
        <field name="Event" type="Event.Event" />
        <field name="Sequence" type="integer" />
        <field name="Price" type="decimal" />
        <field name="Name" type="string" />
        <field name="PriceReduceTaxInc" type="decimal" label="Price Reduce Tax inc" compute="ComputePriceReduceTaxInc" />
        <field name="PriceIncl" type="decimal" label="Price include" compute="ComputePriceIncl" />
        <field name="Product" type="Product.Product" />
        <field name="SaleAvailable" type="bool" compute="ComputeSaleAvailable" />
        <field name="Currency" type="Core.Currency" />
    </model>
</buvi>
