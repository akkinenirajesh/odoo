xml
<buvi>
  <model id="Stock.StockMoveLine" name="StockMoveLine" description="Stock Move Line">
    <field name="ProductQty" type="float" />
    <field name="ProductUom" type="Product.Uom" />
    <field name="LotId" type="Stock.Lot" />
    <field name="LocationId" type="Stock.Location" />
    <field name="LocationDestId" type="Stock.Location" />
    <field name="ProductId" type="Product.Product" />
    <field name="MoveId" type="Stock.Move" />
    <field name="OwnerId" type="Res.Partner" />
    <field name="State" type="Selection" selection="StockMoveLine.States" />
    <field name="Description" type="text" />
    <field name="CompanyID" type="Res.Company" />
    <field name="Date" type="datetime" />
    <field name="PriceUnit" type="float" />
    <field name="Quantity" type="float" />
    <field name="Tracking" type="Selection" selection="StockMoveLine.Tracking" />
  </model>

  <optionset id="StockMoveLine.States" name="Stock Move Line States">
    <option value="draft" name="Draft" />
    <option value="confirmed" name="Confirmed" />
    <option value="assigned" name="Assigned" />
    <option value="done" name="Done" />
    <option value="cancel" name="Cancelled" />
  </optionset>

  <optionset id="StockMoveLine.Tracking" name="Stock Move Line Tracking">
    <option value="none" name="No Tracking" />
    <option value="serial" name="Serial Number" />
    <option value="lot" name="Lot" />
  </optionset>
</buvi>
