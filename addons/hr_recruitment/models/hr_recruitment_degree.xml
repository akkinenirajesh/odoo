<buvi>
    <model id="Hr.RecruitmentDegree" name="RecruitmentDegree" description="Applicant Degree">
        <field name="Name" type="string" label="Degree Name" required="true" translate="true" />
        <field name="Sequence" type="integer" default="1" />
