xml
<buvi>
  <model id="WebsiteEventMeet.EventMeetingRoom" name="EventMeetingRoom" description="Event Meeting Room">
    <field name="Name" type="string" label="Topic" required="true" translate="true" />
    <field name="Active" type="bool" default="true" />
    <field name="IsPublished" type="bool" copy="true" />
    <field name="EventId" type="WebsiteEventMeet.EventEvent" label="Event" required="true" ondelete="cascade" />
    <field name="IsPinned" type="bool" label="Is Pinned" />
    <field name="ChatRoomId" type="Chat.Room" required="true" ondelete="restrict" />
    <field name="RoomMaxCapacity" type="selection" default="8" copy="true" >
      <option value="8" name="8" />
      <option value="16" name="16" />
      <option value="32" name="32" />
      <option value="64" name="64" />
      <option value="128" name="128" />
    </field>
    <field name="Summary" type="string" label="Summary" translate="true" />
    <field name="TargetAudience" type="string" label="Audience" translate="true" />
  </model>
</buvi>
