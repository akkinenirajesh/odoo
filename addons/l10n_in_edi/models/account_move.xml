    <optionset id="Account.L10nInEdiCancelReason" name="L10n In EDI Cancel Reason">
        <option value="1" name="Duplicate" />
        <option value="2" name="DataEntryMistake" />
        <option value="3" name="OrderCancelled" />
        <option value="4" name="Others" />
    </optionset>
</buvi>
