xml
<buvi>
    <model id="Stock.ReportStockQuantity" name="ReportStockQuantity" description="Stock Quantity Report">
        <field name="Date" type="date" label="Date" readonly="true" />
        <field name="ProductTmplId" type="Product.Template" readonly="true" />
        <field name="ProductId" type="Product.Product" label="Product" readonly="true" />
        <field name="State" type="selection" label="State" readonly="true" collection="true">
            <option value="Forecast" name="Forecasted Stock" />
            <option value="In" name="Forecasted Receipts" />
            <option value="Out" name="Forecasted Deliveries" />
        </field>
        <field name="ProductQty" type="float" label="Quantity" readonly="true" />
        <field name="CompanyId" type="Res.Company" readonly="true" />
        <field name="WarehouseId" type="Stock.Warehouse" readonly="true" />
    </model>
</buvi>
