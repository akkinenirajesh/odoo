xml
<buvi>
    <model id="Base.ResUsersDeletion" name="Base.ResUsersDeletion" description="Users Deletion Request">
        <field name="UserId" type="res.users" label="User" ondelete="set null" />
        <field name="UserIdInt" type="integer" label="User Id" compute="ComputeUserIdInt" store="true" />
        <field name="State" type="selection" label="State" required="true" default="todo">
            <option value="todo" name="To Do" />
            <option value="done" name="Done" />
            <option value="fail" name="Failed" />
        </field>
    </model>
</buvi>
