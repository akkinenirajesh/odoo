xml
<buvi>
    <model id="Membership.ReportMembership" name="ReportMembership" description="Membership Analysis">
        <field name="StartDate" type="date" label="Start Date" readonly="true" />
        <field name="DateTo" type="date" label="End Date" readonly="true" />
        <field name="NumWaiting" type="integer" label="# Waiting" readonly="true" />
        <field name="NumInvoiced" type="integer" label="# Invoiced" readonly="true" />
        <field name="NumPaid" type="integer" label="# Paid" readonly="true" />
        <field name="TotPending" type="float" label="Pending Amount" digits="0" readonly="true" />
        <field name="TotEarned" type="float" label="Earned Amount" digits="0" readonly="true" />
        <field name="Partner" type="res.partner" label="Member" readonly="true" />
        <field name="AssociateMember" type="res.partner" label="Associate Member" readonly="true" />
        <field name="Membership" type="product.product" label="Membership Product" readonly="true" />
        <field name="MembershipState" type="selection" label="Current Membership State" readonly="true">
            <option value="none" name="Non Member" />
            <option value="canceled" name="Cancelled Member" />
            <option value="old" name="Old Member" />
            <option value="waiting" name="Waiting Member" />
            <option value="invoiced" name="Invoiced Member" />
            <option value="free" name="Free Member" />
            <option value="paid" name="Paid Member" />
        </field>
        <field name="User" type="res.users" label="Salesperson" readonly="true" />
        <field name="Company" type="res.company" label="Company" readonly="true" />
        <field name="Quantity" type="integer" readonly="true" />
    </model>
</buvi>
