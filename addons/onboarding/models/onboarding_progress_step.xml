xml
<buvi>
    <model id="Onboarding.OnboardingProgressStep" name="Onboarding.OnboardingProgressStep" description="Onboarding Progress Step Tracker">
        <field name="ProgressIds" type="Onboarding.OnboardingProgress" collection="true" />
        <field name="StepState" type="selection" label="Onboarding Step Progress" default="NotDone">
            <option value="NotDone" name="Not Done" />
            <option value="JustDone" name="Just Done" />
            <option value="Done" name="Done" />
        </field>
        <field name="StepId" type="Onboarding.OnboardingStep" required="true" ondelete="cascade" />
        <field name="CompanyId" type="Res.Company" ondelete="cascade" />
    </model>
</buvi>
