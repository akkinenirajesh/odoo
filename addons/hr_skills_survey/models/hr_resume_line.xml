xml
<buvi>
    <model id="HrSkills.ResumeLine" name="ResumeLine" description="Resume Line">
        <field name="DisplayType" type="HrSkills.ResumeLineDisplayType" />
        <field name="DepartmentId" type="Hr.Department" label="Department" />
        <field name="SurveyId" type="Survey.Survey" label="Certification" readonly="true" />
        <field name="ExpirationStatus" type="HrSkills.ExpirationStatus" compute="ComputeExpirationStatus" store="true" />
        <field name="DateEnd" type="DateTime" />
    </model>

    <optionset id="HrSkills.ResumeLineDisplayType" name="Resume Line Display Type">
        <option value="Certification" name="Certification" />
        <!-- Add other existing options here -->
    </optionset>

    <optionset id="HrSkills.ExpirationStatus" name="Expiration Status">
        <option value="Expired" name="Expired" />
        <option value="Expiring" name="Expiring" />
        <option value="Valid" name="Valid" />
    </optionset>
</buvi>
