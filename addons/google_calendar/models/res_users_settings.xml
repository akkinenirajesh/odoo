xml
<buvi>
    <model id="Google.ResUsersSettings" name="ResUsersSettings" description="User Settings for Google Calendar">
        <field name="GoogleCalendarRToken" type="string" label="Refresh Token" groups="Base.GroupSystem" />
        <field name="GoogleCalendarToken" type="string" label="User Token" groups="Base.GroupSystem" />
        <field name="GoogleCalendarTokenValidity" type="datetime" label="Token Validity" groups="Base.GroupSystem" />
        <field name="GoogleCalendarSyncToken" type="string" label="Next Sync Token" groups="Base.GroupSystem" />
        <field name="GoogleCalendarCalId" type="string" label="Calendar ID" groups="Base.GroupSystem"
            help="Last Calendar ID who has been synchronized. If it is changed, we remove all links between GoogleID and Odoo Google Internal ID" />
        <field name="GoogleSynchronizationStopped" type="bool" label="Google Synchronization Stopped" groups="Base.GroupSystem" />
    </model>
</buvi>
