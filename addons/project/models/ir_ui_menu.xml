    <field name="Name" type="string" />
    <field name="Active" type="bool" />
    <field name="Parent" type="Core.IrUiMenu" />
    <field name="Action" type="Core.IrActionsActWindow" />
    <field name="WebIcon" type="string" />
    <field name="Groups" type="Core.ResGroups" collection="true" />
    <field name="Sequence" type="integer" />
    <field name="IsHidden" type="bool" />
    <field name="WebIconData" type="binary" />
    <field name="Color" type="integer" />
  </model>
</buvi>
