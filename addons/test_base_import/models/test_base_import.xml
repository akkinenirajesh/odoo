xml
<buvi>
  <model id="test_base_import.Char" name="test_base_import.Char" description="Tests: Base Import Model, Character">
    <field name="Value" type="string" />
  </model>
  <model id="test_base_import.CharRequired" name="test_base_import.CharRequired" description="Tests: Base Import Model, Character required">
    <field name="Value" type="string" required="true" />
  </model>
  <model id="test_base_import.CharReadonly" name="test_base_import.CharReadonly" description="Tests: Base Import Model, Character readonly">
    <field name="Value" type="string" readonly="true" />
  </model>
  <model id="test_base_import.CharNoreadonly" name="test_base_import.CharNoreadonly" description="Tests: Base Import Model, Character No readonly">
    <field name="Value" type="string" readonly="true" />
  </model>
  <model id="test_base_import.CharStillreadonly" name="test_base_import.CharStillreadonly" description="Tests: Base Import Model, Character still readonly">
    <field name="Value" type="string" readonly="true" />
  </model>
  <model id="test_base_import.M2o" name="test_base_import.M2o" description="Tests: Base Import Model, Many to One">
    <field name="Value" type="test_base_import.M2oRelated" />
  </model>
  <model id="test_base_import.M2oRelated" name="test_base_import.M2oRelated" description="Tests: Base Import Model, Many to One related">
    <field name="Value" type="integer" default="42" />
  </model>
  <model id="test_base_import.M2oRequired" name="test_base_import.M2oRequired" description="Tests: Base Import Model, Many to One required">
    <field name="Value" type="test_base_import.M2oRequiredRelated" required="true" />
  </model>
  <model id="test_base_import.M2oRequiredRelated" name="test_base_import.M2oRequiredRelated" description="Tests: Base Import Model, Many to One required related">
    <field name="Value" type="integer" default="42" />
  </model>
  <model id="test_base_import.O2m" name="test_base_import.O2m" description="Tests: Base Import Model, One to Many" collection="true">
    <field name="Name" type="string" />
    <field name="Value" type="test_base_import.O2mChild" collection="true" />
  </model>
  <model id="test_base_import.O2mChild" name="test_base_import.O2mChild" description="Tests: Base Import Model, One to Many child">
    <field name="Parent_Id" type="test_base_import.O2m" />
    <field name="Value" type="integer" />
  </model>
  <model id="test_base_import.PreviewModel" name="test_base_import.PreviewModel" description="Tests: Base Import Model Preview">
    <field name="Name" type="string" label="Name" />
    <field name="Somevalue" type="integer" label="Some Value" required="true" />
    <field name="Othervalue" type="integer" label="Other Variable" />
  </model>
  <model id="test_base_import.FloatModel" name="test_base_import.FloatModel" description="Tests: Base Import Model Float">
    <field name="Value" type="float" />
    <field name="Value2" type="monetary" />
    <field name="Currency_Id" type="res.currency" />
  </model>
  <model id="test_base_import.ComplexModel" name="test_base_import.ComplexModel" description="Tests: Base Import Model Complex">
    <field name="F" type="float" />
    <field name="M" type="monetary" />
    <field name="C" type="string" />
    <field name="Currency_Id" type="res.currency" />
    <field name="D" type="date" />
    <field name="Dt" type="datetime" />
    <field name="Parent_Id" type="test_base_import.ComplexModel" />
  </model>
</buvi>
