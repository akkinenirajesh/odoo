xml
<buvi>
  <model id="TestXlsxExport.NewModel" name="NewModel" description="Export: Integer">
    <field name="Value" type="integer" default="4" />
    <field name="DisplayName" type="string" />
  </model>
  <model id="TestXlsxExport.ExportAggregator" name="ExportAggregator" description="Export Aggregator">
    <field name="IntSum" type="integer" aggregator="sum" />
    <field name="IntMax" type="integer" aggregator="max" />
    <field name="FloatMin" type="float" aggregator="min" />
    <field name="FloatAvg" type="float" aggregator="avg" />
    <field name="FloatMonetary" type="float" aggregator="sum" currency="CurrencyId" />
    <field name="CurrencyId" type="res.currency" />
    <field name="DateMax" type="date" aggregator="max" />
    <field name="BoolAnd" type="bool" aggregator="bool_and" />
    <field name="BoolOr" type="bool" aggregator="bool_or" />
    <field name="Many2One" type="TestXlsxExport.NewModel" />
    <field name="One2Many" type="TestXlsxExport.ExportAggregatorO2M" collection="true" />
  </model>
  <model id="TestXlsxExport.ExportAggregatorO2M" name="ExportAggregatorO2M" description="Export Aggregator One2Many">
    <field name="ParentId" type="TestXlsxExport.ExportAggregator" />
    <field name="Value" type="integer" />
  </model>
</buvi>
