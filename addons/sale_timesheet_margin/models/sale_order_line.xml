        <field name="ProductId" type="Product.Product" />
        <field name="PurchasePrice" type="float" />
        <field name="Company" type="Res.Company" />
        <field name="ProductUom" type="Product.Uom" />
        <field name="ProjectTimeMode" type="Product.Uom" />
        <field name="CostCurrency" type="Res.Currency" />
