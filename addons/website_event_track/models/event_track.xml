xml
<buvi>
  <model id="WebsiteEventTrack.Track" name="WebsiteEventTrack.Track" description="Event Track">
    <field name="Name" type="string" label="Title" required="true" translate="true" />
    <field name="EventId" type="WebsiteEvent.Event" label="Event" required="true" />
    <field name="Active" type="bool" default="true" />
    <field name="UserId" type="Res.Users" label="Responsible" tracking="true" />
    <field name="CompanyId" type="Res.Company" related="EventId.CompanyId" />
    <field name="TagIds" type="WebsiteEventTrack.TrackTag" collection="true" label="Tags" />
    <field name="Description" type="html" translate="true" sanitizeAttributes="false" sanitizeForm="false" />
    <field name="Color" type="integer" label="Agenda Color" />
    <field name="Priority" type="selection" label="Priority" required="true" default="1">
      <option value="0" name="Low" />
      <option value="1" name="Medium" />
      <option value="2" name="High" />
      <option value="3" name="Highest" />
    </field>
    <field name="StageId" type="WebsiteEventTrack.TrackStage" label="Stage" ondelete="restrict" index="true" copy="false"
      groupExpand="_ReadGroupExpandFull" required="true" tracking="true" />
    <field name="LegendBlocked" type="string" label="Kanban Blocked Explanation" related="StageId.LegendBlocked" readonly="true" />
    <field name="LegendDone" type="string" label="Kanban Valid Explanation" related="StageId.LegendDone" readonly="true" />
    <field name="LegendNormal" type="string" label="Kanban Ongoing Explanation" related="StageId.LegendNormal" readonly="true" />
    <field name="KanbanState" type="selection" label="Kanban State" copy="false" default="normal" required="true">
      <option value="normal" name="Grey" />
      <option value="done" name="Green" />
      <option value="blocked" name="Red" />
    </field>
    <field name="KanbanStateLabel" type="string" label="Kanban State Label" compute="_ComputeKanbanStateLabel" store="true"
      tracking="true" />
    <field name="PartnerId" type="Res.Partner" label="Contact" />
    <field name="PartnerName" type="string" label="Name" compute="_ComputePartnerName" readonly="false" store="true"
      tracking="10" />
    <field name="PartnerEmail" type="string" label="Email" compute="_ComputePartnerEmail" readonly="false" store="true"
      tracking="20" />
    <field name="PartnerPhone" type="string" label="Phone" compute="_ComputePartnerPhone" readonly="false" store="true"
      tracking="30" />
    <field name="PartnerBiography" type="html" label="Biography" compute="_ComputePartnerBiography" sanitizeAttributes="false"
      readonly="false" store="true" />
    <field name="PartnerFunction" type="string" label="Job Position" compute="_ComputePartnerFunction" store="true"
      readonly="false" />
    <field name="PartnerCompanyName" type="string" label="Company Name" compute="_ComputePartnerCompanyName" readonly="false"
      store="true" />
    <field name="PartnerTagLine" type="string" label="Tag Line" compute="_ComputePartnerTagLine" help="Description of the partner (name, function and company name)" />
    <field name="Image" type="image" label="Speaker Photo" compute="_ComputePartnerImage" readonly="false" store="true"
      maxWidth="256" maxHeight="256" />
    <field name="ContactEmail" type="string" label="Contact Email" compute="_ComputeContactEmail" readonly="false" store="true"
      tracking="20" />
    <field name="ContactPhone" type="string" label="Contact Phone" compute="_ComputeContactPhone" readonly="false" store="true"
      tracking="30" />
    <field name="LocationId" type="WebsiteEventTrack.TrackLocation" label="Location" />
    <field name="Date" type="datetime" label="Track Date" />
    <field name="DateEnd" type="datetime" label="Track End Date" compute="_ComputeEndDate" store="true" />
    <field name="Duration" type="float" label="Duration" default="0.5" />
    <field name="IsTrackLive" type="bool" label="Is Track Live" compute="_ComputeTrackTimeData" />
    <field name="IsTrackSoon" type="bool" label="Is Track Soon" compute="_ComputeTrackTimeData" />
    <field name="IsTrackToday" type="bool" label="Is Track Today" compute="_ComputeTrackTimeData" />
    <field name="IsTrackUpcoming" type="bool" label="Is Track Upcoming" compute="_ComputeTrackTimeData" />
    <field name="IsTrackDone" type="bool" label="Is Track Done" compute="_ComputeTrackTimeData" />
    <field name="TrackStartRemaining" type="integer" label="Minutes before track starts" compute="_ComputeTrackTimeData"
      help="Remaining time before track starts (seconds)" />
    <field name="TrackStartRelative" type="integer" label="Minutes compare to track start" compute="_ComputeTrackTimeData"
      help="Relative time compared to track start (seconds)" />
    <field name="WebsiteImage" type="image" label="Website Image" maxWidth="1024" maxHeight="1024" />
    <field name="WebsiteImageUrl" type="string" label="Image URL" compute="_ComputeWebsiteImageUrl" computeSudo="true" />
    <field name="EventTrackVisitorIds" type="WebsiteEventTrack.TrackVisitor" collection="true" label="Track Visitors"
      groups="WebsiteEvent.GroupEventUser" />
    <field name="IsReminderOn" type="bool" label="Is Reminder On" compute="_ComputeIsReminderOn" />
    <field name="WishlistVisitorIds" type="Website.Visitor" collection="true" label="Visitor Wishlist" compute="_ComputeWishlistVisitorIds"
      computeSudo="true" search="_SearchWishlistVisitorIds" groups="WebsiteEvent.GroupEventUser" />
    <field name="WishlistVisitorCount" type="integer" label="# Wishlisted" compute="_ComputeWishlistVisitorIds"
      computeSudo="true" groups="WebsiteEvent.GroupEventUser" />
    <field name="WishlistedByDefault" type="bool" label="Always Wishlisted" help="If set, the talk will be set as favorite for each attendee registered to the event." />
    <field name="WebsiteCta" type="bool" label="Magic Button" help="Display a Call to Action button to your Attendees while they watch your Track." />
    <field name="WebsiteCtaTitle" type="string" label="Button Title" />
    <field name="WebsiteCtaUrl" type="string" label="Button Target URL" />
    <field name="WebsiteCtaDelay" type="integer" label="Show Button" />
    <field name="IsWebsiteCtaLive" type="bool" label="Is CTA Live" compute="_ComputeCtaTimeData"
      help="CTA button is available" />
    <field name="WebsiteCtaStartRemaining" type="integer" label="Minutes before CTA starts" compute="_ComputeCtaTimeData"
      help="Remaining time before CTA starts (seconds)" />
  </model>
</buvi>
