xml
<buvi>
  <model id="PointOfSale.IrUiView" name="IrUiView" description="IrUiView">
    <inherit id="ir.ui.view" />
    <field name="Name" type="string" label="Name" />
    <field name="Active" type="bool" default="true" />
    <field name="Mode" type="string" />
    <field name="Arch" type="text" />
    <field name="ArchFS" type="binary" />
    <field name="ArchDB" type="text" />
    <field name="Key" type="string" />
    <field name="Type" type="selection" selection="['tree', 'form', 'kanban', 'graph', 'calendar', 'pivot', 'search', 'qweb', 'action']" default="tree" />
    <field name="Priority" type="integer" />
    <field name="FieldParent" type="string" />
    <field name="Field" type="string" />
    <field name="GroupsId" type="Core.Group" collection="true" />
    <field name="ModelId" type="Core.Model" />
    <field name="ResModel" type="string" />
    <field name="MenuId" type="Core.Menu" />
    <field name="ParentId" type="Core.IrUiView" />
    <field name="ParentId" type="Core.IrUiView" relation="one2many" collection="true" />
    <field name="inherit_id" type="Core.IrUiView" />
    <field name="inherit_id" type="Core.IrUiView" relation="one2many" collection="true" />
  </model>
</buvi>
