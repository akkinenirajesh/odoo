xml
<buvi>
  <model id="TestConverter.TestModel" name="TestModel" description="Test Converter Model">
    <field name="Char" type="string" />
    <field name="Integer" type="integer" />
    <field name="Float" type="float" />
    <field name="Numeric" type="float" digits="(16, 2)" />
    <field name="Many2One" type="TestConverter.TestModelSub" collection="false" />
    <field name="Binary" type="binary" attachment="false" />
    <field name="Date" type="date" />
    <field name="Datetime" type="datetime" />
    <field name="SelectionStr" type="string">
      <option value="A" name="Qu'il n'est pas arrivé à Toronto" />
      <option value="B" name="Qu'il était supposé arriver à Toronto" />
      <option value="C" name="Qu'est-ce qu'il fout ce maudit pancake, tabernacle ?" />
      <option value="D" name="La réponse D" />
    </field>
    <field name="Html" type="html" />
    <field name="Text" type="text" />
  </model>
  <model id="TestConverter.TestModelSub" name="TestModelSub" description="Subtraction For Test Model & Test Converter">
    <field name="Name" type="string" />
  </model>
  <model id="TestConverter.Monetary" name="Monetary" description="Test Converter Monetary">
    <field name="Value" type="float" digits="(16, 55)" />
  </model>
</buvi>
