        <field name="SaleOrderCount" type="integer" label="Number of Source Sale" groups="SalesTeam.GroupSaleSalesman" />
        <field name="OrderLines" collection="true" type="SalePurchase.PurchaseOrderLine" />
    </model>

    <model id="SalePurchase.PurchaseOrderLine" name="SalePurchase.PurchaseOrderLine" description="Purchase Order Line">
        <field name="SaleOrderId" type="Sale.SaleOrder" label="Sale Order" readonly="true" />
        <field name="SaleLineId" type="Sale.SaleOrderLine" label="Origin Sale Item" />
    </model>
</buvi>
