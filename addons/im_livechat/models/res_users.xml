<buvi>
    <model id="Im.Users" name="Users" description="Update of res.users class for livechat purpose">
        <field name="LivechatUsername" type="string" label="Livechat Username" compute="ComputeLivechatUsername" inverse="InverseLivechatUsername" />
        <field name="LivechatLangIds" type="Core.Lang" label="Livechat Languages" multiple="true" compute="ComputeLivechatLangIds" inverse="InverseLivechatLangIds" />
        <field name="HasAccessLivechat" type="bool" label="Has access to Livechat" compute="ComputeHasAccessLivechat" readonly="true" />
    </model>
</buvi>
