xml
<buvi>
  <model id="Stock.Picking" name="Stock.Picking" description="Picking">
    <field name="Company" type="Res.Company" />
    <field name="PickingTypeId" type="Stock.PickingType" />
    <field name="Partner" type="Res.Partner" />
    <field name="Mobile" type="char" />
    <field name="Phone" type="char" />
    <field name="HasReceivedWarningStockSms" type="bool" />
    <field name="StockMoveSmsValidation" type="bool" />
    <field name="StockSmsConfirmationTemplate" type="Stock.SmsTemplate" />
  </model>
  <model id="Confirm.StockSms" name="Confirm.StockSms" description="Confirm Stock SMS">
    <field name="PickIds" type="Stock.Picking" collection="true" />
  </model>
  <model id="Stock.SmsTemplate" name="Stock.SmsTemplate" description="SMS Template">
    <field name="Name" type="string" label="Name" required="true" translate="true" />
    <field name="Subject" type="char" label="Subject" />
    <field name="Body" type="text" label="Body" />
  </model>
  <model id="Res.Company" name="Res.Company" description="Company">
    <field name="StockMoveSmsValidation" type="bool" />
    <field name="HasReceivedWarningStockSms" type="bool" />
    <field name="StockSmsConfirmationTemplate" type="Stock.SmsTemplate" />
  </model>
  <model id="Stock.PickingType" name="Stock.PickingType" description="Picking Type">
    <field name="Code" type="char" />
  </model>
  <model id="Res.Partner" name="Res.Partner" description="Partner">
    <field name="Mobile" type="char" />
    <field name="Phone" type="char" />
  </model>
</buvi>
